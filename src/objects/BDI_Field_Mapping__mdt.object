<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Batch Data Import takes a single NPSP Data Import record and translates it into multiple records (Contacts, Accounts, Opportunity, Payment, etc.). It does this by mapping fields from the Source Field to the Target Field.</description>
    <fields>
        <fullName>Source_Field__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Source Field</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Target_Field__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Target Field</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>BDI Field Mapping</label>
    <pluralLabel>BDI Field Mappings</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
